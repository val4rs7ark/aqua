		            WHEN '${gYear4}-${gMonth4}-01' <![CDATA[ <= ]]> order_enddate and '${gYear4}-${gMonth4}-31' <![CDATA[ >= ]]> order_enddate THEN '${gMonth4}'
		            WHEN '${gYear5}-${gMonth5}-01' <![CDATA[ <= ]]> order_enddate and '${gYear5}-${gMonth5}-31' <![CDATA[ >= ]]> order_enddate THEN '${gMonth5}'
